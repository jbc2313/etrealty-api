generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("GRID_DATABASE_URL")
}

model Property {
  PropertyId String @id //QueryApi has this as a string. Will have to parse to int.
  ApiUpdateAt String?
  Tags String[]
  PermLink String?
  Status String?
  ListDate String?
  Branding String?
  Photos String[]
  ListPrice Int?
  YearBuilt Int?
  BathsFull Int?
  BathsHalf Int?
  Baths Int?
  LotSqft Int?
  Sqft Int?
  Garage Int?
  Stories Int?
  Beds Int?
  PropertyType String?
  ListingId String?
  PostalCode String?
  State String?
  Longitude Float?
  Latitude Float?
  City String?
  StateCode String?
  Address String?  //Line in ApiQuery
  County String?
}

